{
    "name": "selvi/firebase",
    "description": "Package to integrate firebase with selvi framework",
    "authors": [
        {
            "name": "mochrira",
            "email": "mochrira@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": {
        "mochrira/selvi-framework": {
            "type": "path",
            "url": "../framework",
            "options": {
                "symlink": true
            }
        }
    },
    "require": {
        "kreait/firebase-php": "^5.3",
        "mochrira/selvi-framework": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Selvi\\Firebase\\": "src/"
        }
    }
}
